<script>
  window.pathLib = "<%- lib('') %>";
  window.pathSelfLib = "<%- selfLib('') %>";
  
  var originalConsoleWarn = console.warn;
  console.warn = function (message) {
    if (message.startsWith('Duplicate definition of module')) return;
    originalConsoleWarn.apply(console, arguments);
  };
</script>

<!-- Load monaco-editor -->
<script src="<%- lib('monaco-editor/0.18.1/min/vs/loader.js') %>"></script>
<script src="<%- selfLib('monaco-editor.js') %>"></script>
<link rel="stylesheet" href="<%- selfLib('monaco-editor.css') %>" media="(prefers-color-scheme: no-preference)">
<link rel="stylesheet" href="<%- selfLib('monaco-editor.css') %>" media="(prefers-color-scheme: light)">
<link rel="stylesheet" href="<%- selfLib('monaco-editor-dark.css') %>" media="(prefers-color-scheme: dark)">

<%
this.showLoadingEditor = () => {
%>
<div class="ui active medium text loader">编辑器加载中 …</div>
<%
};
%>
